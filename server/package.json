{
  "name": "take-home-exercise-chrisjagoda",
  "version": "1.0.0",
  "description": "Capital Group software engineer take home exercise",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/capgroup-ig-tech-interviews/take-home-exercise-chrisjagoda.git"
  },
  "author": "Chris Jagoda",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && npm run lint",
    "lint": "eslint \"**/*.{js,ts}\" --quiet --fix",
    "test": "jest --forceExit --coverage --verbose",
    "debug": "npm run build && npm run watch-debug",
    "serve": "node dist/server.js",
    "serve-debug": "nodemon --inspect dist/server.js",
    "setup": "node setup.js",
    "start": "npm run build && npm run serve",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-node": "nodemon dist/server.js",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w -p tsconfig.build.json"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "csv-parser": "^2.3.3",
    "express": "^4.17.1",
    "express-validator": "^6.5.0",
    "sqlite3": "^4.2.0",
    "tinyqueue": "^2.0.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/compression": "^1.0.1",
    "@types/concurrently": "^4.1.0",
    "@types/eslint": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.7.8",
    "@types/sqlite3": "^3.1.6",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "concurrently": "^5.0.0",
    "eslint": "^6.4.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3"
  }
}
